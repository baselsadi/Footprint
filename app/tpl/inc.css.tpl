
#nav 				 {background-color:[~$user.cssZoneA~];}
#nav ul li a 		 {background-color:[~$user.cssZoneB~];}
#nav ul li a:hover   {background-color:[~$user.cssZoneC~]; color:[~$user.cssZoneB~];}
#nav ul li strong a  {background-color:[~$user.cssZoneC~]; color:[~$user.cssZoneB~]; border-bottom-color:[~$user.cssZoneC~];}
#nav2 				 {background-color:[~$user.cssZoneC~]; border-bottom-color:#a1a1a1;}
#nav2 ul li a 		 {color:[~$user.cssZoneD~]; border-bottom-color:[~$user.cssZoneC~];}
#nav2 ul li a:hover  {color:[~$user.cssZoneB~]; border-bottom-color:[~$user.cssZoneD~];}
#nav2 ul li strong a {color:[~$user.cssZoneB~]; border-bottom-color:[~$user.cssZoneD~];}